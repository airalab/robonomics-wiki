{# retutn to main after tests #}
{% set ghLink = "https://github.com/airalab/robonomics-wiki/blob/test-code/src" + page.filePathStem + ".md" %}

{% set ghLastUpdate = github | getCommit(page.fileSlug + ".md")%}

{% if ghLink %}
 <section class="docContribution">
	{% if contributors.length %}
	<div class="page-title-meta">
		<span>Main contributors: </span>
			{% for contributor in contributors %}
				<a target="_blank" href="{{'https://github.com/'+contributor}}"> @{{contributor}}</a>
				{% if not loop.last %}<span>, </span> {% endif %}
			{% endfor %}
	</div>

		<div class="content">
			<div class="github-title">
				<div class="icon">
				{% set icon = "github" %}
					{% include "../icon.njk" %}
				</div>
				<h5>Make a contribution</h5>
			</div>
			<p> Robonomics wiki is open source. See something that's wrong or unclear?
				<a href="{{ghLink}}" class="submit" target="_blank">Submit</a>  a pull request.
			</p>
		</div>

		{% if ghLastUpdate.length %}
		<div class="head">
			Latest <a target="_blank" href="{{ghLastUpdate[0].url}}">commit</a> on {{ghLastUpdate[0].date | readableDate("dd.MM.yyyy")}} by {{ghLastUpdate[0].author}}
		</div>
		{%  endif %}

	{%  endif %}
 </section>
{% endif %}